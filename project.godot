; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="New Game Project"
run/main_scene="res://Utilities/StartUpScreens/TitlePage.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Global="*res://Utilities/Global.gd"
ChatDataMgr="*res://Data/ChatDataMgr.gd"
Signals="*res://Utilities/SignalBus.gd"
TransportMgr="*res://Utilities/TransportMgr.gd"
VocabData="*res://Data/Vocabulary/VocabData.gd"
VocabMgr="*res://Data/Vocabulary/VocabMgr.gd"
InventoryManager="*res://Utilities/Inventory/InventoryManager.gd"
ItemDatabase="*res://Utilities/Inventory/ItemDatabase.gd"
Inventory="*res://Utilities/Inventory/Inventory.tscn"
QuestData="*res://Utilities/Quests/QuestData.gd"
TashaTasksData="*res://Data/Quests/TashaTasksData.gd"
QuestMgr="*res://Utilities/Quests/QuestMgr.gd"
TashaTaskMgr="*res://Data/Quests/TashaTaskMgr.gd"
QuestLog="*res://Utilities/Quests/QuestLog.tscn"
PlantData="*res://Data/PlantData/PlantData.gd"
BooksData="*res://LanguageGames/BookClub/BooksData.gd"
NpcData="*res://Data/NpcData.gd"
ChatMgr="*res://DialogueStuff/ChatMgr.gd"
PopUps="*res://Utilities/UI/PopUps.tscn"
TestAutoload="*res://TestAutoload.gd"
Hud="*res://Utilities/UI/HUD/Hud.tscn"

[debug]

gdscript/warnings/unused_signal=0

[dialogue_manager]

general/states=["Global", "VocabData", "VocabMgr", "InventoryManager", "PlantData", "ItemDatabase", "Inventory", "PopUps", "NpcData"]
general/default_csv_locale="ko"
general/balloon_path="res://DialogueStuff/balloon.tscn"
general/export_characters_in_translation=false
general/wrap_lines=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_width"
window/vsync/vsync_mode=0

[editor]

naming/scene_name_casing=1
naming/script_name_casing=1

[global_group]

Pickupables="Anything that can be picked up and added to inventory"

[gui]

theme/custom_font="res://Assets/Fonts/Maplestory OTF Bold.otf"
theme/default_font_generate_mipmaps=true

[inkgd]

register_templates=true

